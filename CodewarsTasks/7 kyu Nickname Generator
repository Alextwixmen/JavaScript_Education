function nicknameGenerator(name){
    let result; 
    let thirdLet = name.slice(2, 3);
    if(name.length < 4) {
         return  "Error: Name too short"
    }
     else if(thirdLet === "a" || thirdLet === "e" || thirdLet === "i" || thirdLet === "o" || thirdLet === "u"){
        result = name.slice(0,4);
    }
    else{
     result = name.slice(0,3);
     }
   return result;
  }
